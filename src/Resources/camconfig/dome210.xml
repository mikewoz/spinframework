<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE spinframework SYSTEM>
<camConfig>
    <window id="dome view">
        <screen>0</screen>
        <fullscreen>false</fullscreen>
        <windowPosition>50 50</windowPosition>
        <windowSize>50% 50%</windowSize>
        <windowDecoration>true</windowDecoration>
        <useCursor>true</useCursor>
        <supportsResize>true</supportsResize>
        <camera id="domeCamera">
            <spherical>true</spherical>
            <collar>0.95</collar>
            <radius>1.0</radius>
            <textureSize>512</textureSize>
            <viewport>0% 0% 100% 100%</viewport>
            <clearColor>0.5 0.5 0.5 1.0</clearColor>
            <clipping>0.001 1000.0</clipping>
        </camera>
    </window>
</camConfig>
