<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE soundEngine SYSTEM>
<camConfig>
    <window id="split-screen">
        <screen>0</screen>
        <fullscreen>false</fullscreen>
        <windowPosition>50 50</windowPosition>
        <windowSize>800 400</windowSize>
        <clearColor>0 0 0 0</clearColor>
        <windowDecoration>true</windowDecoration>
        <useCursor>true</useCursor>
        <supportsResize>true</supportsResize>
        <camera id="forwardCamera">
            <parent>world</parent>
            <eye>0 0 0</eye>
            <lookat>0 1 0</lookat>
            <up>0 0 1</up>
            <viewport>0% 0% 0% 50% 100%</viewport>
            <clearColor>0.2 0.2 0.2 0</clearColor>
        </camera>
        <camera id="topview">
            <parent>world</parent>
            <eye>0 0 10</eye>
            <lookat>0 0 9</lookat>
            <up>0 1 0</up>
            <viewport>50% 0% 50% 100%</viewport>
            <clearColor>0.2 0 0 0</clearColor>
        </camera>
    </window>
</camConfig>
